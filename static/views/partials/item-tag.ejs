<li class="box" onclick="<%= selectClick %>">
    <a href="#">
        <i class="fa fa-circle-o text-yellow"></i>
        <%= tag %>
        <span class="badge bg-yellow" style="margin-left:10px"><%= tagCount %></span>
    </a>
    <div style="position:absolute;right:10px;top:10px" class="pull-right">
        <button type="button" class="btn btn-box-tool" onclick="<%= removeClick %>">
            <i class="fa fa-times"></i>
        </button>
    </div>
</li>